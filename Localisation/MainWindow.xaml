        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:p="clr-namespace:Localisation.Properties"
        xmlns:local="clr-namespace:Localisation"
        mc:Ignorable="d"
        Title="MainWindow" Height="84.033" Width="273.515">
    <StackPanel>
        <TextBlock Text="{x:Static p:Resources.TxtBlockText}"/>
        <Button Content="{x:Static p:Resources.BtnText}"
                Click="Button_Click"/>
    </StackPanel>
</Window>
